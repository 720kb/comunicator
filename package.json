{
  "name": "comunicator",
  "version": "3.1.0",
  "description": "The 720kb notifier api (atm it uses websockets)",
  "license": "MIT",
  "homepage": "https://github.com/720kb/comunicator",
  "main": "./dist/node/comunicator.js",
  "keywords": [
    "ws",
    "notify",
    "messaging"
  ],
  "bugs": {
    "url": "http://github.com/720kb/comunicator/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/720kb/comunicator.git"
  },
  "dependencies": {
    "@reactivex/rxjs": "^5.2.0",
    "debug": "^2.2.0",
    "joi": "^10.2.2",
    "jsonwebtoken": "^7.3.0",
    "rxjs": "^5.0.0-beta.2",
    "ws": "^2.0.0"
  },
  "devDependencies": {
    "babel-loader": "^6.4.0",
    "babel-preset-env": "^1.1.10",
    "eslint": "^3.17.1",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-babel": "^6.1.1",
    "gulp-cli": "^1.2.2",
    "gulp-eslint": "^3.0.1",
    "gulp-ng-annotate": "^2.0.0",
    "gulp-sourcemaps": "^2.4.1",
    "hapi": "^16.1.0",
    "husky": "^0.13.2",
    "inert": "^4.1.0",
    "jest": "^19.0.2",
    "npm-run-all": "^4.0.2",
    "rimraf": "^2.6.1",
    "run-sequence": "^1.1.5",
    "webpack": "^2.2.1",
    "webpack-validator": "^2.3.0"
  },
  "scripts": {
    "prestart": "npm run build",
    "start": "DEBUG=720kb:* node example/start-node.js",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "clean": "rimraf dist",
    "lint": "gulp lint",
    "validate-webpack:dev": "webpack-validator webpack.config.js --env.dev",
    "validate-webpack:prod": "webpack-validator webpack.config.js --env.prod",
    "validate": "npm-run-all --sequential clean lint validate-webpack:*",
    "build:dev": "webpack --env.dev",
    "build:prod": "webpack --env.prod",
    "build:node": "gulp build",
    "build": "npm-run-all --sequential clean lint build:*",
    "precommit": "npm run validate",
    "preversion": "npm-run-all validate test",
    "version": "git add -A .",
    "postversion": "git push && git push --tags"
  },
  "confs": {
    "js": {
      "src": "src/**/*.js",
      "gulpfile": "gulpfile.js",
      "tasks": "tasks/**/*.js"
    },
    "node": {
      "src": "src/node/**/*.js"
    }
  },
  "author": {
    "name": "Dario Andrei",
    "email": "wouldgo84@gmail.com",
    "url": "https://github.com/wouldgo"
  },
  "contributors": [
    {
      "name": "Filippo Oretti",
      "email": "filippo.oretti@gmail.com",
      "url": "https://github.com/45kb"
    }
  ]
}
