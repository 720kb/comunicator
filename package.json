{
  "name": "comunicator",
  "version": "2.2.3",
  "description": "The 720kb notifier api (atm it uses websockets)",
  "license": "MIT",
  "homepage": "https://github.com/720kb/comunicator",
  "author": {
    "name": "Dario Andrei",
    "email": "wouldgo84@gmail.com",
    "url": "https://github.com/wouldgo"
  },
  "contributors": [
    {
      "name": "Filippo Oretti",
      "email": "filippo.oretti@gmail.com",
      "url": "https://github.com/45kb"
    }
  ],
  "keywords": [
    "ws",
    "notify",
    "messaging"
  ],
  "scripts": {
    "js-min": "./tasks/banner | xargs -0 -I banner uglifyjs src/js/comunicator.js --preamble banner -c -m --screw-ie8 -p relative --source-map dist/comunicator.min.js.map -o dist/comunicator.min.js",
    "ng-min": "./tasks/banner | xargs -0 -I banner uglifyjs src/js/comunicator.js src/angular/comunicator.js --preamble banner -c -m --screw-ie8 -p relative --source-map dist/comunicator-angular.min.js.map -o dist/comunicator-angular.min.js",
    "min": "npm run-script js-min && npm run-script ng-min && plato -r -d report -l .jshintrc -t \"comunicator\" src/",
    "lint": "eslint -c .eslintrc src && jshint -c .jshintrc --reporter=unix src && jscs -c .jscsrc src",
    "test": "lab --debug --verbose --colors spec/*.js",
    "example": "node example/start-node.js",
    "example-debug": "node-debug example/start-node.js",
    "precommit": "npm run-script lint",
    "preversion": "npm run-script lint",
    "version": "npm run-script min && ./tasks/bower-version-bump && git add -A .",
    "postversion": "git push && git push --tags"
  },
  "bugs": {
    "url": "http://github.com/720kb/comunicator/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/720kb/comunicator.git"
  },
  "main": "./src/node/comunicator.js",
  "dependencies": {
    "gulp-rename": "^1.2.2",
    "jsonwebtoken": "^5.4.1",
    "ws": "^1.0.1",
    "zen-observable": "^0.1.9"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.3.13",
    "code": "^2.1.0",
    "del": "^2.2.0",
    "eslint": "^1.10.3",
    "gulp": "^3.9.0",
    "gulp-babel": "^6.1.1",
    "gulp-changed": "^1.3.0",
    "gulp-header": "^1.7.1",
    "gulp-ng-annotate": "^1.1.0",
    "gulp-plumber": "^1.0.1",
    "hapi": "^12.0.1",
    "husky": "^0.10.2",
    "inert": "^3.2.0",
    "jscs": "^2.7.0",
    "jshint": "^2.9.1-rc2",
    "lab": "^8.2.0",
    "node-inspector": "^0.12.5",
    "plato": "^1.5.0",
    "require-dir": "^0.3.0",
    "run-sequence": "^1.1.5",
    "uglify-js": "^2.6.1",
    "vinyl-paths": "^2.1.0"
  }
}
